   <string notr="true">/* 1) Global window background (light-gray gradient) */


QWidget {
    background-color: #73C2FB;; /* Ice Blue*/
}
/* 3) Title label */
QTextEdit{
    color: #FFFFFF;
    font-size: 18px;
    font-weight: 60;
    margin-bottom: 12px;
}

/* 4) Feedback text area */
QTextEdit {
    background-color: #f5f5f5;
    border: 1px solid #dddddd;
    border-radius: 8px;
    padding: 8px;
    color: #555555;
    font-size: 24px;
}
QTextEdit#FeedbackText:focus {
    border: 1px solid #66afe9;
}

/* 5) Star buttons (if you switch to Unicode ★/☆) */
QToolButton#StarButton {
    background: none;
    border: none;
    font-size: 32px;
    color: #cccccc;
    margin: 0 4px;
}
QToolButton#StarButton:hover,
QToolButton#StarButton[checked=&quot;true&quot;] {
    color: #FFD700;   /* gold */
}

/* 6) Submit button */
QPushButton#SubmitButton {
    background-color: #1e90ff;
    color: white;
    border: none;
    border-radius: 8px;
    padding: 12px;
    font-size: 16px;
    margin-top: 16px;
}
QPushButton#SubmitButton:hover {
    background-color: #1c86ee;
}
QPushButton#SubmitButton:pressed {
    background-color: #1874cd;
}
</string>
